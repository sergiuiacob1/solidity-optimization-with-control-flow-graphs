    /*
    *@dev mint Fucntion is to mint the NFTS for public
    @dev _count is the number of NFTS you want to Mint 
    */
    function mint(uint256 _count) public payable {  
        uint256 Supply = totalSupply();
        require(!isBlacklisted[msg.sender], "caller is backlisted"); 
        require(hasSaleStarted == true, "Sale hasn't started");
        require(Supply <= MAX_SUPPLY, "Sold out");
        require(Supply + _count <= MAX_SUPPLY, "Not enough tokens left");
        require(_count <= 20, "Mint 20 or fewer, please.");
        require(msg.value == _count*(floorPrice), "The value submitted with this transaction is too low.");
        payable(owner()).transfer(_count*(floorPrice));         
        _safeMint(msg.sender,_count );
    }

     /*
    *@dev mint Function is to mint the NFTS 
    @dev _count is the number of NFTS you want to Mint 
    */
    function mintOwner(uint256 _count) public onlyOwner {  
        uint256 Supply = totalSupply();
        require(Supply <= MAX_SUPPLY, "Sold out");
        require(Supply + _count <= MAX_SUPPLY, "Not enough tokens left");
        require(_count <= 20, "Mint 20 or fewer, please.");
        _safeMint(msg.sender,_count );
    }
    /*
    *@dev mint Function is to mint the NFTS for whileListed or team members or ogglisted
    @dev _count is the number of NFTS you want to Mint 
    */
    function mintEarly(uint256 _count) public payable {

        uint256 Supply = totalSupply();
        require(Supply <= MAX_SUPPLY, "Sold out");
        require(Supply + _count <= MAX_SUPPLY, "Not enough tokens left");
        require(hasSaleStartedEarly == true, "Sale hasn't started");        
        if(isOggListed(msg.sender)){
            require(_count==1,"Error: Can Not Mint More Than One NFT");
            _safeMint(msg.sender,_count );  
            ogglist[msg.sender] = false;
        } 
        else if(isWhitelisted(msg.sender)){
            require(_count<=2,"Error: Can Not Mint More Than Two NFT");
            require(whitelist[msg.sender].count+_count<=2,"Error: Can Not Mint More Than Two NFT");
            require(msg.value == _count*(floorPrice), "The value submitted with this transaction is too low.");
            if(whitelist[msg.sender].count<2){
                payable(owner()).transfer(_count*(floorPrice));
                _safeMint(msg.sender,_count);
                whitelist[msg.sender].count=whitelist[msg.sender].count+uint8(_count); 
            }else{
               whitelist[msg.sender].islisted = false;
            }
        }
        else{
            require(false,"You Are Not A Valid User");
        }       
    }

    function teamMint(uint256 _count) public onlyTeamlisted{
        uint256 Supply = totalSupply();
        require(Supply <= MAX_SUPPLY, "Sold out");
        require(Supply + _count <= MAX_SUPPLY, "Not enough tokens left");
        require(hasSaleStartedEarly == true, "Sale hasn't started");        
        require(_count<=teamMintNFTS,"Error: Can Not Mint More Than Two NFT");
        require(teamlist[msg.sender].count+_count<=teamMintNFTS,"Error: Can Not Mint More Than Two NFT");
            if(teamlist[msg.sender].count<teamMintNFTS){
                _safeMint(msg.sender,_count);
                teamlist[msg.sender].count=teamlist[msg.sender].count+uint8(_count); 
            }else{
               teamlist[msg.sender].islisted = false;
            }
    }
